#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,b,m;
        cin>>n>>b>>m;
        int last=-1,ans=0;
        int x;
        for(int i=0;i<m;i++)
        {
            cin>>x;
            x=x/b;
            if(last!=x)
            ans++;
            last=x;
        }
        cout<<ans<<endl;
    }
}
